# ~/.config/firejail/xed.local

include globals.local

net none
protocol unix

private-tmp
private-bin xed,xdg-open
private-lib libgtk-3.so,libc.so,libglib-2.0.so
private-etc

read-write ${HOME}

caps.drop all
seccomp
nonewprivs
noroot
nogroups

private-cache
private-dev

noexec /tmp
noexec ${HOME}
